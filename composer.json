{
  "name": "tourze/wechat-mini-program-short-link-bundle",
  "description": "小程序 Short Link",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/security-http": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/wechat-mini-program-bundle": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "WechatMiniProgramShortLinkBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WechatMiniProgramShortLinkBundle\\Tests\\": "tests"
    }
  }
}